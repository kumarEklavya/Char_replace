#include<bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;
    
    char ch1 ,ch2;
    cin>>ch1;
    cin>>ch2;
    string res=" ";
    
    for(char i:s){
        if( i==ch1){
            res = res+ch2;
            
        }
        else if( i==ch2){
            res=res+ch1;
        }
        else{
            res+=i;
        }
    }
    cout<<res<<endl;
}
